<h1>Create New User</h1>
<br>
<%= form_for @user do |f| %>
<%= f.label :Name %>
<%= f.text_field :name %><br>
<%= f.label :Username %>
<%= f.text_field :username %><br>
<%= f.label :Password %>
<%= f.password_field :password %><br>
<%= f.submit %>
<% end %>
<%= button_to "Login", login_path, method: :get %>
<%= button_to "CodeDictionary", codedictionary_path, method: :get %>